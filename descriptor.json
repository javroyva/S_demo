{
    "name": "Upload Cells IA algorithm results to Cells IA viewer",
    "description": "",
    "container-image": {
      "image": "cellsia/upload_cellsia_results",
      "type": ""
    },
    "command-line": "[CYTOMINE_HOST] [CYTOMINE_PUBLIC_KEY] [CYTOMINE_PRIVATE_KEY] [CYTOMINE_ID_PROJECT] [CYTOMINE_ID_SOFTWARE] [PROJECT_NAME]",
    "inputs": [
      {
        "id": "cytomine_host",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine host",
        "set-by-server": true,
        "optional": false,
        "type": "String"
      },
      {
        "id": "cytomine_public_key",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine public key",
        "set-by-server": true,
        "optional": false,
        "type": "String"
      },
      {
        "id": "cytomine_private_key",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine private key",
        "set-by-server": true,
        "optional": false,
        "type": "String"
      },
      {
        "id": "cytomine_id_project",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine project ID",
        "set-by-server": true,
        "optional": false,
        "type": "Number"
      },
      {
        "id": "cytomine_id_software",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine software ID",
        "set-by-server": true,
        "optional": false,
        "type": "Number"
      },
      {
        "id": "project_name",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Project name",
        "set-by-server": false,
        "optional": false,
        "type": "String"
  }
    ],
  
    "schema-version": "cytomine-0.1"
  }
